{
    "manifest_version": 3,
    "name": "Is this AI?",
    "version": "0.2",
    "permissions": [
        "activeTab",
        "contextMenus", 
        "storage"
    ],
    "host_permissions": [
        "<all_urls>",
        "http://localhost:5000/"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}
